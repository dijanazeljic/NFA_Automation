							"    let stlJobID = jsonData.jobId;",
							"    pm.environment.set(\"STLJOBID\" + counter, stlJobID);",
							"    let STLJOBID= pm.environment.get(\"STLJOBID\" + counter);",
							"    console.log(STLJOBID);",
							"    postman.setNextRequest(\"action - Search To List\");",
							"    console.log(\"COUNTER: \" + counter++);",
							"    pm.globals.set(\"counter\", counter++);",
							"   ",
