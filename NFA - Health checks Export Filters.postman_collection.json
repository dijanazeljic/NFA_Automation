							"    console.log(\"EXPfilter\",EXPfilter);",
							"    if (EXPfilter && EXPfilter.length > 0) {",
							"    let expJobID = jsonData.jobId;",
							"    pm.environment.set(\"EXPJOBID\", expJobID);",
