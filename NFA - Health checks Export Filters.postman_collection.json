							"var EXPfilters = pm.environment.get(\"EXPfilters\");",
							"if (!EXPfilters) {",
							"   EXPfilters = [\"{ range(company.size, 0, 5) }\", \"{ has(company.industry) }\", \"{ isIn(company.country, 'United States') }\"];",
							"}",
							"console.log(\"EXPprfil\",EXPfilters);",
							"var EXPcurrentFilter = EXPfilters.shift();",
							"pm.environment.set('EXPfilter', EXPcurrentFilter);",
							"pm.environment.set('EXPfilters', EXPfilters);",
							""
